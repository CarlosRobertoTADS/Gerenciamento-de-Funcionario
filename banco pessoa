-- -----------------------------------------------------
-- Table `projectspring`.`pessoa`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `projectspring`.`pessoa` (
  `id_pessoa` INT(11) NOT NULL AUTO_INCREMENT,
  `ds_nome` VARCHAR(100) NOT NULL,
  `fl_ativo` BIT(1) NOT NULL,
